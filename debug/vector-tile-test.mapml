<mapml>
    <head>
        <title>World Countries - WGS84</title>
        <meta charset="utf-8" />
        <meta name="zoom" content="min=0,max=5" />
        <link rel="license" href="https://www.nrcan.gc.ca/earth-sciences/geography/topographic-information/free-data-geogratis/licence/17285" title="Â© Natural Resources Canada" />
        <link rel="stylesheet" href="vector-tile-test.css" />
    </head>
    <body>
        <extent units="WGS84">
            <input name="zoomLevel" type="zoom" min="0" max="2" value="0"/>

            <input name="row" type="location" axis="row" units="tilematrix" min="0" max="2"/>
            <input name="col" type="location" axis="column" units="tilematrix" min="0" max="2"/>

            <link rel='tile' type='text/mapml' title='Tiles for ne_10m_admin_0_countries (as MapML)' tref='../test/e2e/data/tiles/wgs84/{zoomLevel}/r{row}_c{col}.mapml'/>

        </extent>
    </body>
</mapml>
